function iCheckAll(){
    //checkbox
    var $chk_all = $(".tbl_list th .icheckbox_minimal");
    var $chk_item = $(".tbl_list td .icheckbox_minimal");
    
    $chk_all.on('ifChecked', function(){
        $chk_item.iCheck("check");
    });
    $chk_all.on('ifUnchecked', function(){
        $chk_item.iCheck("uncheck");
    })
}
</script>
