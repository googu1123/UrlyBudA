/*통계 요약*/
.clear input[type="radio"]{display:none; margin:0; padding:0;}
.clear #radio{float:right; text-align:center; line-height:23px;}
.clear #radio a{display:block; float:left; width:39px; height:23px; padding:0; margin:0; font-size:11px; border:1px solid #ccc; text-decoration:none;}
.clear #radio a.first{border-radius:2px 0 0 2px; border-right:0;}
.clear #radio a.last{border-radius:0 2px 2px 0; border-left:0;}
.clear #radio a.on{border:1px solid #0078af; background:#00a0e9; color:#fff;}
.clear #radio input[type="text"].date{display:inline-block; width:180px; height:23px; line-height:23px; margin-left:10px; padding:0 10px; border:1px solid #ddd; text-align:center;}

.calLayout, .calLayout *{box-sizing:content-box; -webkit-box-sizing:content-box;}

.chart_li .thonth{color:#ff5400; border-bottom:0;}
.chart_li .bg{background:#fcfcfc; color:#888;}
.chart_li .bgft{color:#00a0e9;}
.chart_li .ftwb{font-weight:bold;}
.chart_li .child{padding-left:50px;}
.chart_li .up{background:url(../img/arrow_up.png) 70px 14px no-repeat;}
.chart_li .down{background:url(../img/arrow_down.png) 70px 14px no-repeat;}
.chart_li td.bgtd{font-weight:bold; background-color:#fff; border-right:1px solid #ddd;}

.chart_tab{width:100%; height:40px; text-align:center; line-height:40px; background:url(../img/bg_tabline.png) 0 bottom repeat-x;}
.chart_tab li{float:left; width:120px; height:38px; color:#999; font-weight:bold; border:1px solid #ccc; border-bottom:0;}
.chart_tab li.on{height:40px; color:#778dd5; border:2px solid #778dd5; border-bottom:0; background:#fff;}
.chart_tab li a{text-decoration:none; color:#999;}
.chart_tab li a:hover,
.chart_tab li a:focus{color:#666;}

.chart_view .chart_info{padding-top:20px; padding-bottom:10px; border-bottom:1px solid #e5e5e5;}
.chart_view .chart_date{float:left; font-weight:bold;}
.chart_view .chart_area{width:100%; height:240px; margin-bottom:20px;}

