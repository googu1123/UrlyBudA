	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
    <bean id="dataSource" class="org.apache.commons.dbcp.BasicDataSource" destroy-method="close">
                
		<!-- ============================== -->
		<!-- 			Oracle 				-->
		<!-- ============================== -->
		<property name="validationQuery" value="select 1 from dual"/>
		<property name="driverClassName" value="oracle.jdbc.driver.OracleDriver"/>
		<property name="url" value="jdbc:oracle:thin:@(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=192.168.217.183)(PORT=1521))(CONNECT_DATA=(SERVER=DEDICATED)(SERVICE_NAME = ORCL)))"/>
		<property name="username" value="icms_admin"/>
		<property name="password" value="icms_admin"/>
         
    </bean>
    
